<div class="container">
    <center><h1>Welcome to Java's Campground!</h1></center>
    <br>
</div>
<div class="container text-center">
    <div class="row">
        <%for(camp of camps){%>
            <div class="col">
                <div class="card" style="width: 18rem;">
                    <img src="<%=camp.image%>" class="card-img-top" alt="Camp Image">
                    <div class="card-body">
                        <h5 class="card-title">
                            <%=camp.title%>
                        </h5>
                        <p class="card-text">
                            <%=camp.description%>
                        </p>
                        <a href="#" class="btn btn-primary">
                            <%=camp.location%>
                        </a>
                    </div>
                </div>
            </div>
            <%}%>
    </div>
</div>